import streamlit as st
import matplotlib.pyplot as plt
import numpy as np

# Contoh plot sederhana
x = np.linspace(0, 10, 100)
y = np.sin(x)

# Menggunakan matplotlib untuk membuat plot
plt.plot(x, y)
plt.xlabel('X-axis')
plt.ylabel('Y-axis')
plt.title('Plot Sederhana')

# Menampilkan plot menggunakan Streamlit
st.pyplot()

# Contoh penggunaan lainnya
st.write("Ini adalah teks yang ditulis menggunakan Streamlit.")
